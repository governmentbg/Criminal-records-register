@model string
@using Infragistics.Web.Mvc
@using MJ_CAIS.Common.Constants
@using MJ_CAIS.WebPortal.Public.Controllers
@using MJ_CAIS.WebPortal.Public.Models.Application
@using MJ_CAIS.WebSetup.Utils.CustomHtmlHelpers
@using MJ_CAIS.Common.Resources
@using MJ_CAIS.Common

@{
    var title = ApplicationResources.titleAppPreview;
    ViewData["Title"] = title;
}
<svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
    <symbol id="exclamation-triangle-fill" fill="currentColor" viewBox="0 0 16 16">
        <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
    </symbol>
</svg>
<div class="col-12">
    <div class="alert alert-warning d-flex align-items-center my-0" role="alert">
        <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Предупреждение: дубллирано заявление!"><use xlink:href="#exclamation-triangle-fill" /></svg>
        <div>
            Заявление със същата цел вече е подадено! Може да го прегледате <a asp-controller="Application" asp-action="Preview" asp-route-id="@Model">тук</a>.
        </div>
    </div>
</div>