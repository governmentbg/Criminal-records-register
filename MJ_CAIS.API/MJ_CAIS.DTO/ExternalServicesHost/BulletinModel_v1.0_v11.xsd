				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DCH-00-O">
				<xs:annotation>
					<xs:documentation>Помилване</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
