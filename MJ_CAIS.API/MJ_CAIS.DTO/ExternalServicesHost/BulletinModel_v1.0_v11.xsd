				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DCH-00-X">
				<xs:annotation>
					<xs:documentation>Друго</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DCH-88-A">
