{
  "OracleSQLCompatibility": "11",
  "ConnectionStrings": {
    "CaisConnectionStringT": "Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=172.16.0.183)(PORT=1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=MJCAIST)));User ID=MJ_CAIS;Password=***REMOVED***;",
    "CaisConnectionString": "Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=172.31.12.87)(PORT=1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=MJCAISD)));User ID=MJ_CAIS;Password=***REMOVED***;"

  },
  "BNBPayments": {
    "regex1": "(\\D|^)(?<code>\\d{11})(\\D|$)",
    "regex2": "(\\D|^)(?<code>\\d{11,})(\\D|$)"
  },
  "EventListenerConfiguration": {
    "ServiceInstallerSettings": {
      "description": "Event listener - listening for incoming messages from BNB",
      "displayName": "TL BNB Event Listener",
      "serviceName": "TL BNB Event Listener"
    },
    "EventLogSettings": {
      "eventLogSource": "TL_BNB_Event_Listener",
      "eventLogName": "TL_BNB_Event_Listener_Log\\"
    },
    "WatcherSettingsList": {
      "WatcherSettings": {
        "key": "CaseWatcherSettings",
        "elapsedTimeInSeconds": "30",
        "fileFilter": "*.txt",
        "deleteCompletedFiles": "false",
        "maxFileProcess": "1",
        "sourcePath": "Files\\",
        "compleatePath": "..\\FilesCompleted\\",
        "processingPath": "..\\FilesProcessing\\",
        "errorPath": "..\\FilesError\\"
      } 
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "logs/BNBFileProcessor-${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "logfile"
      }
    ]
  },

  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ]
}